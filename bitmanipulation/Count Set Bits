

// User function Template for Java
class Solution {
      
    static int setBits(int N) {
       int c=0;
       while(N>0){
           c+=(N&1);
           N=N>>1;
       }
       return c;
    }
}



// User function Template for Java
class Solution {
    static int setBits(int N) {
        int c=0;
        while(N!=0){
           N=N&(N-1);
           c+=1;
        }
        return c;
        // code here
    }
}
